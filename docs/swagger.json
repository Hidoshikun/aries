{
    "swagger": "2.0",
    "info": {
        "description": "Aries 开源博客项目 API 接口文档",
        "title": "Gin Swagger",
        "contact": {},
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8088",
    "paths": {
        "/api/v1/auth/captcha": {
            "get": {
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "授权"
                ],
                "summary": "创建验证码",
                "responses": {
                    "100": {
                        "description": "Continue",
                        "schema": {
                            "$ref": "#/definitions/util.Result"
                        }
                    },
                    "104": {
                        "schema": {
                            "$ref": "#/definitions/util.Result"
                        }
                    }
                }
            }
        },
        "/api/v1/auth/login": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "授权"
                ],
                "summary": "登录",
                "parameters": [
                    {
                        "description": "登录表单",
                        "name": "loginForm",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/form.LoginForm"
                        }
                    }
                ],
                "responses": {
                    "100": {
                        "description": "Continue",
                        "schema": {
                            "$ref": "#/definitions/util.Result"
                        }
                    },
                    "104": {
                        "schema": {
                            "$ref": "#/definitions/util.Result"
                        }
                    }
                }
            }
        },
        "/api/v1/auth/register": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "授权"
                ],
                "summary": "注册",
                "parameters": [
                    {
                        "description": "注册表单",
                        "name": "regForm",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/form.RegisterForm"
                        }
                    }
                ],
                "responses": {
                    "100": {
                        "description": "Continue",
                        "schema": {
                            "$ref": "#/definitions/util.Result"
                        }
                    },
                    "104": {
                        "schema": {
                            "$ref": "#/definitions/util.Result"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "form.LoginForm": {
            "type": "object",
            "required": [
                "captcha_id",
                "captcha_val",
                "pwd",
                "username"
            ],
            "properties": {
                "captcha_id": {
                    "description": "验证码 ID",
                    "type": "string"
                },
                "captcha_val": {
                    "description": "验证码",
                    "type": "string"
                },
                "pwd": {
                    "description": "密码",
                    "type": "string"
                },
                "username": {
                    "description": "用户名",
                    "type": "string"
                }
            }
        },
        "form.RegisterForm": {
            "type": "object",
            "required": [
                "email",
                "pwd",
                "second_pwd",
                "username"
            ],
            "properties": {
                "email": {
                    "description": "邮箱",
                    "type": "string"
                },
                "pwd": {
                    "description": "密码",
                    "type": "string"
                },
                "second_pwd": {
                    "description": "确认密码",
                    "type": "string"
                },
                "username": {
                    "description": "用户名",
                    "type": "string"
                }
            }
        },
        "util.Result": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "状态码",
                    "type": "integer",
                    "example": 0
                },
                "data": {
                    "description": "数据",
                    "type": "object"
                },
                "msg": {
                    "description": "信息",
                    "type": "string",
                    "example": "信息"
                }
            }
        }
    }
}